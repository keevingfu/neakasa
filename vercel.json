{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "CI": "false",
    "ESLINT_NO_DEV_ERRORS": "true",
    "TSC_COMPILE_ON_ERROR": "true",
    "DISABLE_ESLINT_PLUGIN": "true"
  },
  "build": {
    "env": {
      "CI": "false",
      "ESLINT_NO_DEV_ERRORS": "true",
      "TSC_COMPILE_ON_ERROR": "true",
      "DISABLE_ESLINT_PLUGIN": "true"
    }
  }
}